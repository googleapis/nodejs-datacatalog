{
  "repository": "googleapis/nodejs-datacatalog",
  "name": "@google-cloud/datacatalog",
  "version": "1.6.0",
  "author": "Google LLC",
  "description": "Google Cloud Data Catalog API client for Node.js",
  "main": "build/src/index.js",
  "files": [
    "build/src",
    "build/protos",
    "AUTHORS",
    "COPYING"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google datacatalog",
    "datacatalog",
    "Google Cloud Data Catalog API"
  ],
  "dependencies": {
    "google-gax": "^1.7.5"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.0.3",
    "c8": "^7.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "gts": "^1.1.0",
    "jsdoc": "^3.6.2",
    "jsdoc-fresh": "^1.0.1",
    "jsdoc-region-tag": "^1.0.2",
    "linkinator": "^1.4.0",
    "mocha": "^6.1.4",
    "pack-n-play": "^1.0.0-2",
    "prettier": "^1.17.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "docs": "jsdoc -c .jsdoc.js",
    "fix": "gts fix && eslint . --fix",
    "lint": "gts check && eslint .",
    "test": "c8 mocha build/test/*.js",
    "predocs-test": "npm run docs",
    "docs-test": "linkinator docs",
    "samples-test": "c8 mocha samples/test/*.js",
    "system-test": "c8 mocha --timeout=5000 build/system-test/*.js",
    "compile": "tsc -p . && cp -r protos build/",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.10.0"
  }
}
